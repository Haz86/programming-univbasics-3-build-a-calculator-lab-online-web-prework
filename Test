def addition(num1, num2)
  
  num1 + num2
  
end

def subtraction(num1, num2)
  
  num1 - num2

end

def division(num1, num2)
  
  num1 / num2

end

def multiplication(num1, num2)
  
  num1 * num2

end

def modulo(num1, num2)
  
  num1 % num2

end

def square_root(num)

Math.sqrt(num)


end

puts addition(4,5)
puts subtraction(5,4)
puts division(10,2)
puts multiplication(5,5)
puts modulo(2,7)
puts square_root(81)
